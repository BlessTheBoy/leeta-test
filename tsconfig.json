{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "baseUrl": ".",
    "paths": {
      "@constants/*": ["./src/shared/utils/constants/*"],
      "@components/*": ["./src/shared/components/*"],
      "@assets/*": ["./src/shared/assets/*"],
      "@vectors/*": ["./src/shared/vectors/*"],
      "@styles/*": ["./src/shared/styles/*"],
      "@screens/*": ["./src/screens/*"],
      "@navigator/*": ["./src/navigator/*"],
      "@/*": ["src/*"],
      "@root/*": ["./*"]
    }
  }
}
